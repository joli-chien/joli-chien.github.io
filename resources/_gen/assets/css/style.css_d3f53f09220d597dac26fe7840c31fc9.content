.gallery-row{margin-bottom:2rem}.card-body a{text-decoration:none;color:#000}.card-body a:hover{color:#8a2be2}.extraMedia{display:block;margin:auto;width:37.5%;min-width:20em}img{display:flex;width:30%}.media{width:30%;min-width:20em;margin:auto;padding-bottom:10em}.content{padding-left:10%;padding-right:10%;font-family:barlow,sans-serif}.content h1{padding-top:1em;font-family:montserrat,sans-serif;text-transform:uppercase;padding-bottom:1em;font-size:2em}.cultureBackground{background:linear-gradient(to bottom,#6b566d 0%,#c98a8d 97%);filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#6b566d',endColorstr='#c98a8d',GradientType=0 );color:#fdebf3}.navbar{background:#fff;position:fixed;top:0;width:100%;opacity:1;z-index:1;margin:auto;font-family:montserrat,sans-serif;list-style-type:none;font-size:.9em}.navbar-nav{position:fixed;opacity:1;z-index:1;width:100%;margin:auto;background:#fff}.navText{color:#f16278}.navText:hover{color:#a03747}nav ul{display:flex;padding:.5rem;width:100%}nav ul li{flex:1;text-align:center;justify-content:space-around;width:100%;text-transform:uppercase}footer{background:#fff;padding-bottom:2em}footer h1{color:#f16278;text-transform:uppercase;font-family:montserrat,sans-serif;padding:.5em;text-align:center;padding-top:1em;font-size:2em}footer p{color:#8c5c74;font-family:barlow,sans-serif;text-align:center;line-height:.7em}#socialmedia a{color:#f16278;font-size:2em}#socialmedia a:hover{color:#a03747}